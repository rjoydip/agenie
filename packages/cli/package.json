{
  "name": "@orbit/cli",
  "version": "0.0.0",
  "description": "Orbit - An autonomous AI agent CLI powered by LangChain",
  "bin": {
    "orbit": "./src/index.tsx"
  },
  "type": "module",
  "main": "src/index.tsx",
  "exports": {
    "./*": "./src/*.tsx",
    ".": "./src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.tsx",
    "dev": "bun --watch run src/index.tsx"
  },
  "dependencies": {
    "@orbit/agent": "workspace:*",
    "@orbit/ui": "workspace:*",
    "@orbit/utils": "workspace:*",
    "ink": "catalog:",
    "react": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/bun": "catalog:dev",
    "@types/react": "catalog:dev",
    "typescript": "catalog:dev"
  }
}
