{
  "name": "@agenie/cli",
  "version": "0.0.0",
  "description": "CLI for Agenie",
  "bin": {
    "agenie": "dist/index.js"
  },
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    "./*": "./src/*.tsx",
    ".": "./src/index.tsx"
  },
  "scripts": {
    "typecheck": "tsgo --noEmit",
    "start": "bun run src/index.tsx",
    "dev": "bun --watch run src/index.tsx",
    "build": "bun build src/index.tsx --outdir dist --target bun --minify"
  },
  "dependencies": {
    "@agenie/agent": "workspace:*",
    "@agenie/ui": "workspace:*",
    "@agenie/utils": "workspace:*",
    "ink": "catalog:",
    "react": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/bun": "catalog:dev",
    "@types/react": "catalog:dev",
    "typescript": "catalog:dev"
  }
}
