{
  "name": "@agenie/agent",
  "version": "0.0.0",
  "description": "Agent package for Agenie",
  "type": "module",
  "main": "src/index.tsx",
  "exports": {
    "./*": "./src/*.ts",
    ".": "./src/index.ts",
    "./message-history": "./src/message-history.ts",
    "./orchestrator": "./src/orchestrator.ts",
    "./prompts": "./src/prompts.ts",
    "./schemas": "./src/schemas.ts",
    "./state": "./src/state.ts",
    "./task-executor": "./src/task-executor.ts",
    "./tool-context": "./src/tool-context.ts",
    "./tool-executor": "./src/tool-executor.ts",
    "./types": "./src/types.ts",
    "./hooks/*": "./src/hooks/*.ts",
    "./hooks": "./src/hooks/index.ts",
    "./hooks/useAgentExecution": "./src/hooks/useAgentExecution.ts",
    "./hooks/useQueryQueue": "./src/hooks/useQueryQueue.ts",
    "./models/*": "./src/models/*.ts",
    "./models": "./src/models/index.ts",
    "./models/llm": "./src/models/llm.ts",
    "./models/ollama": "./src/models/ollama.ts",
    "./phases/*": "./src/phases/*.ts",
    "./phases": "./src/phases/index.ts",
    "./phases/answer": "./src/phases/answer.ts",
    "./phases/execute": "./src/phases/execute.ts",
    "./phases/plan": "./src/phases/plan.ts",
    "./phases/reflect": "./src/phases/reflect.ts",
    "./phases/understand": "./src/phases/understand.ts",
    "./tools/*": "./src/tools/*.ts",
    "./tools": "./src/tools/index.ts",
    "./tools/search": "./src/tools/search.ts"
  },
  "scripts": {
    "typecheck": "tsgo --noEmit",
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts"
  },
  "dependencies": {
    "@agenie/utils": "workspace:*",
    "@ai-sdk/anthropic": "3.0.9",
    "@ai-sdk/google": "3.0.6",
    "@ai-sdk/openai": "3.0.7",
    "ai": "6.0.27",
    "react": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/bun": "catalog:dev",
    "@types/react": "catalog:dev"
  }
}
